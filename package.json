{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript node.js api",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint --ext .ts ./src ./test",
    "lint:fix": "eslint --ext .ts ./src ./test --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cleberpereiradev/node-typescript-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Cleber Pereira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cleberpereiradev/node-typescript-api/issues"
  },
  "homepage": "https://github.com/cleberpereiradev/node-typescript-api#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "module-alias": "^2.2.3"
  }
}
